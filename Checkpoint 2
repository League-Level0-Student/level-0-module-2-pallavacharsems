int randomNumber = randomMaker.nextInt(100);
int x = randomMaker.nextInt(100);
System.out.println(randomNumber + " + " + x + " = " + (randomNumber-x));
System.out.println(randomNumber + " - " + x + " = " + (randomNumber-x));

